<?xml version="1.0"?>
<rdf:RDF xmlns="http://irkb.com/ir-ontology#"
     xml:base="http://irkb.com/ir-ontology"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:terms="http://purl.org/dc/terms/">
    <owl:Ontology rdf:about="http://irkb.com/ir-ontology">
        <terms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-06-01</terms:created>
        <terms:creator>Haula Galadima, Cormac Doherty, Rob Brennan</terms:creator>
        <terms:description>An ontology for modeling cybersecurity incident response processes</terms:description>
        <terms:license rdf:resource="https://creativecommons.org/licenses/by/4.0/"/>
        <terms:title>Incident Response Ontology</terms:title>
        <owl:versionInfo>1.0</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://purl.org/dc/terms/created -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/created"/>
    


    <!-- http://purl.org/dc/terms/creator -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/creator"/>
    


    <!-- http://purl.org/dc/terms/description -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/description"/>
    


    <!-- http://purl.org/dc/terms/license -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/license"/>
    


    <!-- http://purl.org/dc/terms/title -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/title"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.w3.org/2001/XMLSchema#date -->


    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#date"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://irkb.com/ir-ontology#assignedToCoa -->


    <owl:ObjectProperty rdf:about="http://irkb.com/ir-ontology#assignedToCoa">
        <rdfs:domain rdf:resource="http://irkb.com/ir-ontology#Analyst"/>
        <rdfs:range rdf:resource="http://irkb.com/ir-ontology#CourseOfAction"/>
        <rdfs:comment>Inverse of hasCoaAssignedAnalyst. Links an analyst to the courses of action they are assigned to execute.</rdfs:comment>
        <rdfs:label>assigned to COA</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://irkb.com/ir-ontology#confirmedIncident -->


    <owl:ObjectProperty rdf:about="http://irkb.com/ir-ontology#confirmedIncident">
        <rdfs:domain rdf:resource="http://irkb.com/ir-ontology#Analyst"/>
        <rdfs:range rdf:resource="http://irkb.com/ir-ontology#Incident"/>
        <rdfs:comment>Inverse of hasConfirmationAnalyst. Links an analyst to the incidents they confirmed.</rdfs:comment>
        <rdfs:label>confirmed incident</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://irkb.com/ir-ontology#createdArtefact -->


    <owl:ObjectProperty rdf:about="http://irkb.com/ir-ontology#createdArtefact">
        <rdfs:domain rdf:resource="http://irkb.com/ir-ontology#Analyst"/>
        <rdfs:range rdf:resource="http://irkb.com/ir-ontology#Artefact"/>
        <rdfs:comment>Inverse of hasArtefactCreationAnalyst. Links an analyst to the artefacts they created.</rdfs:comment>
        <rdfs:label>created artefact</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://irkb.com/ir-ontology#createdCoa -->


    <owl:ObjectProperty rdf:about="http://irkb.com/ir-ontology#createdCoa">
        <rdfs:domain rdf:resource="http://irkb.com/ir-ontology#Analyst"/>
        <rdfs:range rdf:resource="http://irkb.com/ir-ontology#CourseOfAction"/>
        <rdfs:comment>Inverse of hasCoaCreationAnalyst. Links an analyst to the courses of action they created.</rdfs:comment>
        <rdfs:label>created COA</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://irkb.com/ir-ontology#generated -->


    <owl:ObjectProperty rdf:about="http://irkb.com/ir-ontology#generated">
        <rdfs:domain rdf:resource="http://irkb.com/ir-ontology#SecurityEvent"/>
        <rdfs:range rdf:resource="http://irkb.com/ir-ontology#SecurityAnalysis"/>
        <rdfs:comment>Inverse of prov:wasGeneratedBy. Indicates a security event generated an analysis.</rdfs:comment>
        <rdfs:label>generated</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://irkb.com/ir-ontology#generatedChat -->


    <owl:ObjectProperty rdf:about="http://irkb.com/ir-ontology#generatedChat">
        <rdfs:domain rdf:resource="http://irkb.com/ir-ontology#Analyst"/>
        <rdfs:range rdf:resource="http://irkb.com/ir-ontology#ResponderCommunication"/>
        <rdfs:comment>Inverse of hasChatAnalyst. Links an analyst to the chat they generated.</rdfs:comment>
        <rdfs:label>generated chat</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://irkb.com/ir-ontology#hadAssociatedEntity -->


    <owl:ObjectProperty rdf:about="http://irkb.com/ir-ontology#hadAssociatedEntity">
        <rdfs:domain rdf:resource="http://irkb.com/ir-ontology#Incident"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://irkb.com/ir-ontology#Analyst"/>
                    <rdf:Description rdf:about="http://irkb.com/ir-ontology#Artefact"/>
                    <rdf:Description rdf:about="http://irkb.com/ir-ontology#SecurityEvent"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment>Inverse of prov:wasAssociatedWith. Indicates an incident had an associated entity.</rdfs:comment>
        <rdfs:label>had associated entity</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://irkb.com/ir-ontology#hasAnalysis -->


    <owl:ObjectProperty rdf:about="http://irkb.com/ir-ontology#hasAnalysis">
        <owl:inverseOf rdf:resource="http://irkb.com/ir-ontology#isAnalysisOf"/>
        <rdfs:domain rdf:resource="http://irkb.com/ir-ontology#Incident"/>
        <rdfs:range rdf:resource="http://irkb.com/ir-ontology#SecurityAnalysis"/>
        <rdfs:comment>Links an incident to the security analyses performed on it.</rdfs:comment>
        <rdfs:label>has analysis</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://irkb.com/ir-ontology#hasAnalysisPerformingAnalyst -->


    <owl:ObjectProperty rdf:about="http://irkb.com/ir-ontology#hasAnalysisPerformingAnalyst">
        <owl:inverseOf rdf:resource="http://irkb.com/ir-ontology#performedAnalysis"/>
        <rdfs:domain rdf:resource="http://irkb.com/ir-ontology#SecurityAnalysis"/>
        <rdfs:range rdf:resource="http://irkb.com/ir-ontology#Analyst"/>
        <rdfs:comment>Links a security analysis to the analyst who performed it.</rdfs:comment>
        <rdfs:label>has analysis performing analyst</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://irkb.com/ir-ontology#hasArtefact -->


    <owl:ObjectProperty rdf:about="http://irkb.com/ir-ontology#hasArtefact">
        <owl:inverseOf rdf:resource="http://irkb.com/ir-ontology#isArtefactOf"/>
        <rdfs:domain rdf:resource="http://irkb.com/ir-ontology#Incident"/>
        <rdfs:range rdf:resource="http://irkb.com/ir-ontology#Artefact"/>
        <rdfs:comment>Links an incident to related artefacts/observables collected during the investigation.</rdfs:comment>
        <rdfs:label>has artefact</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://irkb.com/ir-ontology#hasArtefactCreationAnalyst -->


    <owl:ObjectProperty rdf:about="http://irkb.com/ir-ontology#hasArtefactCreationAnalyst">
        <owl:inverseOf rdf:resource="http://irkb.com/ir-ontology#createdArtefact"/>
        <rdfs:domain rdf:resource="http://irkb.com/ir-ontology#Artefact"/>
        <rdfs:range rdf:resource="http://irkb.com/ir-ontology#Analyst"/>
        <rdfs:comment>Links an artefact to the analyst who collected, created, or documented it.</rdfs:comment>
        <rdfs:label>has artefact creation analyst</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://irkb.com/ir-ontology#hasChatAnalyst -->


    <owl:ObjectProperty rdf:about="http://irkb.com/ir-ontology#hasChatAnalyst">
        <owl:inverseOf rdf:resource="http://irkb.com/ir-ontology#generatedChat"/>
        <rdfs:domain rdf:resource="http://irkb.com/ir-ontology#ResponderCommunication"/>
        <rdfs:range rdf:resource="http://irkb.com/ir-ontology#Analyst"/>
        <rdfs:comment>Links a communication to the analyst who made the chat.</rdfs:comment>
        <rdfs:label>has chat analyst</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://irkb.com/ir-ontology#hasCoa -->


    <owl:ObjectProperty rdf:about="http://irkb.com/ir-ontology#hasCoa">
        <owl:inverseOf rdf:resource="http://irkb.com/ir-ontology#isCoaOf"/>
        <rdfs:domain rdf:resource="http://irkb.com/ir-ontology#Incident"/>
        <rdfs:range rdf:resource="http://irkb.com/ir-ontology#CourseOfAction"/>
        <rdfs:comment>Links an incident to the response actions (COA) taken to resolve it.</rdfs:comment>
        <rdfs:label>has course of action</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://irkb.com/ir-ontology#hasCoaAssignedAnalyst -->


    <owl:ObjectProperty rdf:about="http://irkb.com/ir-ontology#hasCoaAssignedAnalyst">
        <owl:inverseOf rdf:resource="http://irkb.com/ir-ontology#assignedToCoa"/>
        <rdfs:domain rdf:resource="http://irkb.com/ir-ontology#CourseOfAction"/>
        <rdfs:range rdf:resource="http://irkb.com/ir-ontology#Analyst"/>
        <rdfs:comment>Links a course of action to the analyst responsible for executing it.</rdfs:comment>
        <rdfs:label>has COA assigned analyst</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://irkb.com/ir-ontology#hasCoaCreationAnalyst -->


    <owl:ObjectProperty rdf:about="http://irkb.com/ir-ontology#hasCoaCreationAnalyst">
        <owl:inverseOf rdf:resource="http://irkb.com/ir-ontology#createdCoa"/>
        <rdfs:domain rdf:resource="http://irkb.com/ir-ontology#CourseOfAction"/>
        <rdfs:range rdf:resource="http://irkb.com/ir-ontology#Analyst"/>
        <rdfs:comment>Links a course of action to the analyst who created or planned it.</rdfs:comment>
        <rdfs:label>has COA creation analyst</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://irkb.com/ir-ontology#hasConfirmationAnalyst -->


    <owl:ObjectProperty rdf:about="http://irkb.com/ir-ontology#hasConfirmationAnalyst">
        <owl:inverseOf rdf:resource="http://irkb.com/ir-ontology#confirmedIncident"/>
        <rdfs:domain rdf:resource="http://irkb.com/ir-ontology#Incident"/>
        <rdfs:range rdf:resource="http://irkb.com/ir-ontology#Analyst"/>
        <rdfs:comment>Links an incident to the analyst who confirmed it as a genuine security incident.</rdfs:comment>
        <rdfs:label>has confirmation analyst</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://irkb.com/ir-ontology#hasEvent -->


    <owl:ObjectProperty rdf:about="http://irkb.com/ir-ontology#hasEvent">
        <owl:inverseOf rdf:resource="http://irkb.com/ir-ontology#isEventOf"/>
        <rdfs:domain rdf:resource="http://irkb.com/ir-ontology#Incident"/>
        <rdfs:range rdf:resource="http://irkb.com/ir-ontology#SecurityEvent"/>
        <rdfs:comment>Links an incident to one or more related security events that triggered or are part of it.</rdfs:comment>
        <rdfs:label>has event</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://irkb.com/ir-ontology#hasPertainingChat -->


    <owl:ObjectProperty rdf:about="http://irkb.com/ir-ontology#hasPertainingChat">
        <rdfs:domain rdf:resource="http://irkb.com/ir-ontology#Incident"/>
        <rdfs:range rdf:resource="http://irkb.com/ir-ontology#ResponderCommunication"/>
        <rdfs:comment>Inverse of pertainsToIncident. Links an incident to a chat about it.</rdfs:comment>
        <rdfs:label>has pertaining chat</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://irkb.com/ir-ontology#isAnalysisOf -->


    <owl:ObjectProperty rdf:about="http://irkb.com/ir-ontology#isAnalysisOf">
        <rdfs:domain rdf:resource="http://irkb.com/ir-ontology#SecurityAnalysis"/>
        <rdfs:range rdf:resource="http://irkb.com/ir-ontology#Incident"/>
        <rdfs:comment>Inverse of hasAnalysis. Links an analysis back to its incident.</rdfs:comment>
        <rdfs:label>is analysis of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://irkb.com/ir-ontology#isArtefactOf -->


    <owl:ObjectProperty rdf:about="http://irkb.com/ir-ontology#isArtefactOf">
        <rdfs:domain rdf:resource="http://irkb.com/ir-ontology#Artefact"/>
        <rdfs:range rdf:resource="http://irkb.com/ir-ontology#Incident"/>
        <rdfs:comment>Inverse of hasArtefact. Links an artefact back to its incident.</rdfs:comment>
        <rdfs:label>is artefact of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://irkb.com/ir-ontology#isCoaOf -->


    <owl:ObjectProperty rdf:about="http://irkb.com/ir-ontology#isCoaOf">
        <rdfs:domain rdf:resource="http://irkb.com/ir-ontology#CourseOfAction"/>
        <rdfs:range rdf:resource="http://irkb.com/ir-ontology#Incident"/>
        <rdfs:comment>Inverse of hasCoa. Links a course of action back to its incident.</rdfs:comment>
        <rdfs:label>is COA of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://irkb.com/ir-ontology#isEventOf -->


    <owl:ObjectProperty rdf:about="http://irkb.com/ir-ontology#isEventOf">
        <rdfs:domain rdf:resource="http://irkb.com/ir-ontology#SecurityEvent"/>
        <rdfs:range rdf:resource="http://irkb.com/ir-ontology#Incident"/>
        <rdfs:comment>Inverse of hasEvent. Links a security event back to its parent incident.</rdfs:comment>
        <rdfs:label>is event of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://irkb.com/ir-ontology#performed -->


    <owl:ObjectProperty rdf:about="http://irkb.com/ir-ontology#performed">
        <rdfs:domain rdf:resource="http://irkb.com/ir-ontology#Analyst"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://irkb.com/ir-ontology#SecurityAnalysis"/>
                    <rdf:Description rdf:about="http://irkb.com/ir-ontology#SecurityEvent"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment>Inverse of prov:wasPerformedBy. Indicates an analyst performed an activity.</rdfs:comment>
        <rdfs:label>performed</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://irkb.com/ir-ontology#performedAnalysis -->


    <owl:ObjectProperty rdf:about="http://irkb.com/ir-ontology#performedAnalysis">
        <rdfs:domain rdf:resource="http://irkb.com/ir-ontology#Analyst"/>
        <rdfs:range rdf:resource="http://irkb.com/ir-ontology#SecurityAnalysis"/>
        <rdfs:comment>Inverse of hasAnalysisPerformingAnalyst. Links an analyst to the analyses they performed.</rdfs:comment>
        <rdfs:label>performed analysis</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://irkb.com/ir-ontology#pertainsToIncident -->


    <owl:ObjectProperty rdf:about="http://irkb.com/ir-ontology#pertainsToIncident">
        <owl:inverseOf rdf:resource="http://irkb.com/ir-ontology#hasPertainingChat"/>
        <rdfs:domain rdf:resource="http://irkb.com/ir-ontology#ResponderCommunication"/>
        <rdfs:range rdf:resource="http://irkb.com/ir-ontology#Incident"/>
        <rdfs:comment>Links a chat to the incident it discusses.</rdfs:comment>
        <rdfs:label>pertains to incident</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://irkb.com/ir-ontology#wasUsedBy -->


    <owl:ObjectProperty rdf:about="http://irkb.com/ir-ontology#wasUsedBy">
        <rdfs:domain rdf:resource="http://irkb.com/ir-ontology#SecurityAnalysis"/>
        <rdfs:range rdf:resource="http://irkb.com/ir-ontology#CourseOfAction"/>
        <rdfs:comment>Inverse of prov:used. Indicates an analysis was used by a course of action.</rdfs:comment>
        <rdfs:label>was used by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/prov#used -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/prov#used">
        <owl:inverseOf rdf:resource="http://irkb.com/ir-ontology#wasUsedBy"/>
        <rdfs:domain rdf:resource="http://irkb.com/ir-ontology#CourseOfAction"/>
        <rdfs:range rdf:resource="http://irkb.com/ir-ontology#SecurityAnalysis"/>
        <rdfs:comment>Indicates that a course of action used specific analysis</rdfs:comment>
        <rdfs:label>used</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/prov#wasAssociatedWith -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/prov#wasAssociatedWith">
        <owl:inverseOf rdf:resource="http://irkb.com/ir-ontology#hadAssociatedEntity"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://irkb.com/ir-ontology#Analyst"/>
                    <rdf:Description rdf:about="http://irkb.com/ir-ontology#Artefact"/>
                    <rdf:Description rdf:about="http://irkb.com/ir-ontology#SecurityEvent"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://irkb.com/ir-ontology#Incident"/>
        <rdfs:comment>Indicates association with an incident</rdfs:comment>
        <rdfs:label>was associated with</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/prov#wasGeneratedBy -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/prov#wasGeneratedBy">
        <owl:inverseOf rdf:resource="http://irkb.com/ir-ontology#generated"/>
        <rdfs:domain rdf:resource="http://irkb.com/ir-ontology#SecurityAnalysis"/>
        <rdfs:range rdf:resource="http://irkb.com/ir-ontology#SecurityEvent"/>
        <rdfs:comment>Indicates the security event that generated this analysis</rdfs:comment>
        <rdfs:label>was generated by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/prov#wasPerformedBy -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/prov#wasPerformedBy">
        <owl:inverseOf rdf:resource="http://irkb.com/ir-ontology#performed"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://irkb.com/ir-ontology#SecurityAnalysis"/>
                    <rdf:Description rdf:about="http://irkb.com/ir-ontology#SecurityEvent"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://irkb.com/ir-ontology#Analyst"/>
        <rdfs:comment>Indicates the analyst who performed this activity</rdfs:comment>
        <rdfs:label>was performed by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://irkb.com/ir-ontology#hasAnalystEmail -->


    <owl:DatatypeProperty rdf:about="http://irkb.com/ir-ontology#hasAnalystEmail">
        <rdfs:domain rdf:resource="http://irkb.com/ir-ontology#Analyst"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Email address of the analyst.</rdfs:comment>
        <rdfs:label>has analyst email</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://irkb.com/ir-ontology#hasAnalystId -->


    <owl:DatatypeProperty rdf:about="http://irkb.com/ir-ontology#hasAnalystId">
        <rdfs:domain rdf:resource="http://irkb.com/ir-ontology#Analyst"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Unique identifier for an analyst.</rdfs:comment>
        <rdfs:label>has analyst ID</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://irkb.com/ir-ontology#hasArtefactCreationDateTime -->


    <owl:DatatypeProperty rdf:about="http://irkb.com/ir-ontology#hasArtefactCreationDateTime">
        <rdfs:domain rdf:resource="http://irkb.com/ir-ontology#Artefact"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>Timestamp when the artefact was collected.</rdfs:comment>
        <rdfs:label>has artefact creation datetime</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://irkb.com/ir-ontology#hasArtefactDataValue -->


    <owl:DatatypeProperty rdf:about="http://irkb.com/ir-ontology#hasArtefactDataValue">
        <rdfs:domain rdf:resource="http://irkb.com/ir-ontology#Artefact"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>The actual content or value of the artefact .</rdfs:comment>
        <rdfs:label>has artefact data value</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://irkb.com/ir-ontology#hasArtefactType -->


    <owl:DatatypeProperty rdf:about="http://irkb.com/ir-ontology#hasArtefactType">
        <rdfs:domain rdf:resource="http://irkb.com/ir-ontology#Artefact"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Classification of the artefact.</rdfs:comment>
        <rdfs:label>has artefact type</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://irkb.com/ir-ontology#hasChatMessage -->


    <owl:DatatypeProperty rdf:about="http://irkb.com/ir-ontology#hasChatMessage">
        <rdfs:domain rdf:resource="http://irkb.com/ir-ontology#ResponderCommunication"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>The content or body of the communication message.</rdfs:comment>
        <rdfs:label>has chat message</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://irkb.com/ir-ontology#hasChatMessageDateTime -->


    <owl:DatatypeProperty rdf:about="http://irkb.com/ir-ontology#hasChatMessageDateTime">
        <rdfs:domain rdf:resource="http://irkb.com/ir-ontology#ResponderCommunication"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>Timestamp when the communication occurred.</rdfs:comment>
        <rdfs:label>has chat message datetime</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://irkb.com/ir-ontology#hasChatSentiment -->


    <owl:DatatypeProperty rdf:about="http://irkb.com/ir-ontology#hasChatSentiment">
        <rdfs:domain rdf:resource="http://irkb.com/ir-ontology#ResponderCommunication"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>The sentiment or tone of the communication.</rdfs:comment>
        <rdfs:label>has chat sentiment</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://irkb.com/ir-ontology#hasCoaCreationDateTime -->


    <owl:DatatypeProperty rdf:about="http://irkb.com/ir-ontology#hasCoaCreationDateTime">
        <rdfs:domain rdf:resource="http://irkb.com/ir-ontology#CourseOfAction"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>Timestamp when the course of action was created.</rdfs:comment>
        <rdfs:label>has COA creation datetime</rdfs:label>
    </owl:DatatypeProperty>
    


